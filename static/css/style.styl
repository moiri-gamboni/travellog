#container
	position:absolute
	height:100%
	width:100%
	#whiteblur
		height:auto
		width:100%
		position:absolute
		background-color:white
		opacity: 0.2
		#sidenav
			background-color:white
			opacity:0.2
			position:absolute
			#sidenav-top
				width:100%
				height:50px
				top:0
				border-bottom:solid 1px white
			#sidenav-bottom
				width:100%
				height:50px
				bottom:0
				border-top:solid 1px white
			#sidenav-left
				width:50px
				height:100%
				left:0
				border-right:solid 1px white
			#sidenav-right
				height:100%
				width:50px
				right:0
				border-left:solid 1px white
