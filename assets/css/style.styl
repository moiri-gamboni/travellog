[ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
  display: none !important;
}
*, 
*:after,
*::before
	box-sizing:border-box
*::-webkit-scrollbar 
	width: 3px
*::-webkit-scrollbar-track
	background-color: transparent
*::-webkit-scrollbar-track-piece 
	background-color: transparent
*::-webkit-scrollbar-thumb 
	height: 15px 
	background-color: #999
	border-radius: 15px
*::-webkit-scrollbar-corner
	background-color: transparent

// for google maps
#loading
	position: absolute
	z-index: 20
	height:100px
	bottom: 50px
	left: 50%
	margin-left: -50px
	pointer-events: none
	&.big
		font-size:100px
		height: 200px
	&.hide-all
		opacity:0
	&.hide-1 li:nth-child(1), &.hide-2 li:nth-child(2), &.hide-3 li:nth-child(3), &.hide-4 li:nth-child(4)
		visibility: hidden
	&.center
		bottom: 20%
	&.corner
		bottom: 70px
		left: 120px
	&.small
		font-size: 30px
		height: 15px

#minimap
	height: 25%
	width: calc(100% - 60px)
	position: absolute
	z-index: 1
	top: 30px
	left: 30px
	#map-canvas
		height: 100%
#add-map-canvas
	height: 40%
	width: 100%
	bottom: -50px
	position: absolute
	display: none
	&.show
		display: block



.move-up
	top: 100px
.fade
	opacity: 1;
	transition: opacity .5s ease-in-out;
	-moz-transition: opacity .5s ease-in-out;
	-webkit-transition: opacity .5s ease-in-out;
.slow-fade
	opacity: 1;
	transition: opacity 1.5s ease-in-out;
	-moz-transition: opacity 1.5s ease-in-out;
	-webkit-transition: opacity 1.5s ease-in-out;
.fadeout
	opacity:0
.fadein
	opacity:1
.background
	height:100%
	width:100%
	top:0
	left:0
	position:absolute
	background-size:cover
	background-position:center
	background-repeat:no-repeat
	&.background-0
		background: url('/static/img/1280calmlake.jpg')
		background-size:cover	
	&.background-1
		background: url('/static/img/1280eveningsky.jpg')
	&.background-2
		background: url('/static/img/1280roughwaves.jpg')
	&.background-3
		background: url('/static/img/1280snowymountain.jpg')
		background-repeat:no-repeat
		background-size:cover	
	&.passive
		opacity:0

#launch-screen
	height:100%
	width:100%
	top:0
	left:0
	position:absolute
	font-family: "Roboto", sans-serif
	#logo
		width: 400px
		height: 120px
		position: absolute
		left: 50%
		margin-left: -200px
		top: 50%
		margin-top: -60px
	hr
		height: 15px
		width: 170px
		background: #292929
		border: transparent
		top: 48%
		left: 50%
		position: absolute
		margin-left: -85px
		&.top-bar
			margin-top: -125px
	h1
		font-size: 3em
		position: absolute
		top: 50%
		margin-top: -80px
		left: 50%
		margin-left: -130px
		font-weight: 100
		text-transform:uppercase
		color:#292929
	p
		font-size: 1.5em
		position: absolute
		top: 50%
		left: 50%
		margin-left: -270px
		font-weight:300
		text-align:center
		color:#666
	h3
		position: absolute
		font-size: 2em
		bottom: 50px
		margin-top: 100px
		left: calc(50% - 200px)
		font-weight: 300
		text-transform: uppercase
		opacity: 0
		text-align: center;
		width: 400px
		height: 80px
	h1:hover, h3:hover
		cursor: pointer
	h3
		&.fadein
			opacity: 0.6
			&:hover
				opacity: 1
#container
	font-family: "Roboto", sans-serif
	position:absolute
	overflow-x:hidden
	overflow-y:hidden
	height:100%
	width:100%
	top:0
	left:0
	background: url('/static/img/boat.jpg')
	background-position:center
	background-repeat:no-repeat
	background-size:cover
	transition: opacity .5s ease-in-out
	-moz-transition: opacity .5s ease-in-out
	-webkit-transition: opacity .5s ease-in-out
	&.hide
		opacity:0
		pointer-events:none
	#loading
		position:absolute
		height:100%
		width:100%
		bottom: 50px
		left:0
		visibility:hidden
		span
			position: absolute
			color: white
			font-size: 2em
			top: 50%
			margin-top: 57px
			left: 50%
			margin-left: -57px
		img
			top:50%
			position:absolute
			margin-left:-16px
			left:50%
	#overlay
		position:absolute
		height:100%
		width:100%
		opacity:0
		z-index : 11
		&.fadein
			opacity: 1
			pointer-events:auto
		pointer-events:none
		color: white
		transition:.5s ease-in-out
		-moz-transition: .5s ease-in-out
		-webkit-transition: .5s ease-in-out
		
		#escape
			position: absolute
			top: 0
			width: 50px
			height: 50px
			opacity: 0.8
			&:hover
				opacity: 1
			right: 40px
			font-size: 2em
			cursor: pointer
		#blackblur
			height:100%
			width:100%
			background:black
			z-index: 1
			opacity:0.9
		h2
			font-size: 1em
		#overlay-content
			position: absolute
			top: 0
			width: calc(100% - 100px)
			height: calc(100% - 130px)
			margin-left: 50px
			margin-top: 50px
			opacity:0
			&.fadein
				opacity: 1
				pointer-events:auto
			pointer-events:none
			z-index:1	
			.message
				font-size: 1.5em;
				position: absolute;
				left: 50%;
				text-align: center;
				width: 290px;
				margin-left: -145px;
				.white
					color: white
				.small
					font-size: 0.9em
				.link
					font-size: 0.5em
				&.push-down
					top: 25%;
			#complete
				button
					top: 150%
			button
				background:transparent
				border:transparent
				outline:transparent
				color:#fff
				font-size:1.5em
				padding:0px
				position:absolute
				top:60%
				right:10px
				opacity: 0.7
				&:hover
					cursor: pointer
					opacity: 1
			span
				font-size:1.3em
			p
				font-size: 1em
				color: #aaa
				position: absolute
				top: -30px
			#search
				margin-top:0px
				color:#aaa
				width: 100%;
				margin-left: 0;
				text-align: left;
				left: 0;
				button
					top: 70%
				.active
  					color:#ed4d4a
  					cursor: pointer
				input
					border:none
					background:transparent
					font-size:1.5em
					border-bottom:1px solid #aaa
					outline:none
					margin-top: 10px
					padding-left: 10px
					color:#aaa
				ul
					padding-left: 0px
					overflow: scroll
					height: 150px
					width: 300px
					li
						list-style:none
						line-height:2em
						opacity: 0.8
						cursor: pointer
						&:hover
							opacity: 1
						&.active
							opacity: 1




	.log-details
		width: calc(100% - 100px)
		height: calc(80% - 180px)
		padding-bottom: 0px
		top: calc(25% + 50px)
		z-index: 10
		position: absolute
		margin-left: 50px
		padding: 0px
		&.launch
			left:-200%	
		.log-title
			text-transform: uppercase
			font-size: 1.8em
			color: white
		.log-author
			padding: 10px 0px
			color:#ed4d4a
			font-size:1.5em
			text-transform:uppercase
		.log-content
			color: white
			line-height: 1.3em
			font-size:1em
			height: calc(100% - 50px)
			img
				width: 100%
				height: auto
		#log-wrapper
			height: 95%
			overflow-y: scroll
			overflow-x: hidden
			padding-right: 30px
	#blackshade
		height:100%
		width:100%
		position:absolute
		background-color:black
		opacity:0.6
	#nav
		padding-right: 70px
		padding-bottom: 60px
		position:absolute
		bottom: 0
		right: 0

		img
			height:30px
			margin-right:10px
			opacity:0.5
			&:hover
				opacity:1
				transition: opacity .25s ease-in-out
				-moz-transition: opacity .25s ease-in-out
				-webkit-transition: opacity .25s ease-in-out
				cursor:pointer

	#country
		text-transform: uppercase
		font-size: 1.5em
		color: #999
		position: absolute
		width: 250px
		right: 75px
		margin-top: 110px
		text-align: right
		-webkit-transition:all 0.2s ease-in-out;
		-moz-transition:all 0.2s ease-in-out;
		-o-transition:all 0.2s ease-in-out;
		transition:all 0.2s ease-in-out;
	.sidenav
		background:rgba(255,255,255,0.4)
		z-index: 2
		position:absolute
		-webkit-transition:all 0.2s ease-in-out
		-moz-transition:all 0.2s ease-in-out
		-o-transition:all 0.2s ease-in-out
		transition:all 0.2s ease-in-out
		&:hover
			background:rgba(255,255,255,0.9)
			cursor:pointer
	.sidenav-top
		width:100%
		height:30px
		top:0
		border-bottom:solid 1px white
	.sidenav-bottom
		width:100%
		height:30px
		bottom:0
		border-top:solid 1px white
	.sidenav-left
		width:30px
		height:100%
		left:0
		border-right:solid 1px white
	.sidenav-right
		height:100%
		width:30px
		right:0
		border-left:solid 1px white
	.arrow
		width: 0
		height: 0
		position: absolute
		opacity: 0.8
		&.arrow-up-down
			border-left: 15px solid transparent
			border-right: 15px solid transparent
			right:50%
			margin-right:-10px
		&.arrow-left-right
			border-top: 15px solid transparent
			border-bottom: 15px solid transparent
			top:50%
			margin-top:-10px
		&.arrow-up		
			border-bottom: 15px solid #fff
			margin-top: 5px
		&.arrow-down
			border-top: 15px solid #fff
			bottom:0
			margin-bottom: 5px
		&.arrow-right
			border-left: 15px solid #fff
			right:0
			margin-right: 5px
		&.arrow-left 
			border-right:15px solid #fff
			left:0
			margin-left: 5px


@media (min-width:320px)
	#launch-screen
		p
			font-size: 1.5em
			width: 300px
			margin-top: 50px
			margin-left: -150px
			text-align:center
		h3
			margin-top: 90px
	#container
		.log-details
			padding: 0px
			.log-title
				font-size: 1.7em

@media (min-width:480px)
	#container 
		.log-details
			.log-content
				margin-top: -5px



@media (min-width:760px)
	#minimap
		width: calc(100% - 100px)
		top: 50px
		left: 50px
	#container
		.log-details
			padding: 30px 30px 30px 50px
			.log-content
				margin-top:20px
				img
					padding: 0px 100px
			.log-title
				font-size:2.2em
		#overlay-content
			p
				margin-left:auto
				margin-right:auto
			#search	
				ul
					height: 200px
				button
					font-size:2em

		#country
			font-size: 2em
			margin-top: 60px
		.sidenav-top
			height:50px
		.sidenav-bottom
			height:50px
		.sidenav-left
			width:50px
		.sidenav-right
			width:50px
		.arrow
			&.arrow-up-down
				border-left: 20px solid transparent
				border-right: 20px solid transparent
				right:50%
				margin-right:-10px
			&.arrow-left-right
				border-top: 20px solid transparent
				border-bottom: 20px solid transparent
				top:50%
				margin-top:-10px
			&.arrow-up		
				border-bottom: 20px solid #fff
				top: 0
				margin-top: 15px
			&.arrow-down
				border-top: 20px solid #fff
				bottom:0
				margin-bottom: 15px
			&.arrow-right
				border-left: 20px solid #fff
				right:0
				margin-right: 15px
			&.arrow-left 
				border-right:20px solid #fff
				left:0
				margin-left: 15px

@media (min-height: 1000px)
	.background
		&.background-0
			background: url('/static/img/2000calmlake.jpg')
		&.background-1
			background: url('/static/img/2000eveningsky.jpg')
		&.background-2
			background: url('/static/img/2000roughwaves.jpg')
		&.background-3
			background: url('/static/img/2000snowymountain.jpg')

@media (max-height: 550px)
	.message
		top: 10%
	#launch-screen
		h3
			bottom: 0px
	#loading
		bottom: 10px
